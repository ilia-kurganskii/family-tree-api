{
  "name": "family-tree-api",
  "version": "0.0.2",
  "description": "Family Tree API service",
  "author": "Ilya Kurganskii",
  "license": "MIT",
  "keywords": [
    "NestJS",
    "Typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/elijah-kurganskiy/family-tree-api.git"
  },
  "bugs": {
    "url": "https://github.com/elijah-kurganskiy/family-tree-api/issues"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --collect-coverage",
    "test:e2e": "jest --config jest-e2e.config.ts --collect-coverage --runInBand --forceExit",
    "docker:e2e": "docker-compose -f docker-compose.e2e.yml -p e2e up --abort-on-container-exit --exit-code-from app --renew-anon-volumes",
    "docker:build": "docker-compose build",
    "docker:dev": "docker-compose  -f docker-compose.dev.yml up --force-recreate --build -d",
    "docker:dev:stop": "docker-compose  -f docker-compose.dev.yml down ",
    "docker:dev:uninstall": "docker-compose  -f docker-compose.dev.yml down -v"
  },
  "dependencies": {
    "@casl/ability": "^5.4.3",
    "@nestjs/common": "^8.2.4",
    "@nestjs/config": "1.1.6",
    "@nestjs/core": "^8.2.4",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.0.1",
    "@nestjs/platform-express": "8.2.4",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/typeorm": "^8.0.3",
    "apollo-server-express": "3.6.1",
    "bcrypt": "5.0.0",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "cls-hooked": "4.2.2",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.28.0",
    "graphql": "15.8.0",
    "graphql-tools": "8.2.0",
    "mongodb": "^3.7.3",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "^7.5.1",
    "swagger-ui-express": "4.1.5",
    "typeorm": "^0.2.41",
    "uuid": "8.3.2",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@apollo/gateway": "0.42.3",
    "@nestjs/cli": "7.5.4",
    "@nestjs/testing": "8.2.4",
    "@types/bcrypt": "3.0.0",
    "@types/chance": "1.1.0",
    "@types/cls-hooked": "4.3.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "4.17.8",
    "@types/jest": "26.0.19",
    "@types/mongodb": "^4.0.7",
    "@types/node": "13.13.5",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "chance": "1.1.7",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-prettier": "3.2.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "supertest": "5.0.0",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.4",
    "ts-morph": "^13.0.2",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.5.2"
  }
}

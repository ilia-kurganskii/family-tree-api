{
  "name": "family-tree-api",
  "version": "0.0.2",
  "description": "Family Tree API service",
  "author": "Ilya Kurganskii",
  "license": "MIT",
  "keywords": [
    "NestJS",
    "Prisma",
    "Prisma Client",
    "Typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/elijah-kurganskiy/family-tree-api.git"
  },
  "bugs": {
    "url": "https://github.com/elijah-kurganskiy/family-tree-api/issues"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "migrate:dev:apply": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --collect-coverage",
    "test:e2e": "jest --config jest-e2e.config.ts --collect-coverage --runInBand",
    "docker:e2e": "docker-compose -f docker-compose.e2e.yml -p e2e up --abort-on-container-exit --exit-code-from app --renew-anon-volumes",
    "docker:build": "docker-compose build",
    "docker:dev": "docker-compose  -f docker-compose.dev.yml up --force-recreate --build -d",
    "docker:prisma:generate": "docker-compose  -f docker-compose.dev.yml exec family-tree-api npm run prisma:generate",
    "docker:migrate:dev:create": "docker-compose  -f docker-compose.dev.yml exec family-tree-api npm run migrate:dev:create",
    "docker:migrate:dev:apply": "docker-compose  -f docker-compose.dev.yml exec family-tree-api npm run migrate:dev:apply",
    "docker:dev:stop": "docker-compose  -f docker-compose.dev.yml down ",
    "docker:dev:uninstall": "docker-compose  -f docker-compose.dev.yml down -v"
  },
  "dependencies": {
    "@casl/ability": "^5.2.2",
    "@devoxa/prisma-relay-cursor-connection": "1.1.0",
    "@nestjs/common": "7.6.11",
    "@nestjs/config": "0.6.1",
    "@nestjs/core": "7.6.1",
    "@nestjs/graphql": "7.9.1",
    "@nestjs/jwt": "7.2.0",
    "@nestjs/passport": "7.1.5",
    "@nestjs/platform-express": "7.6.1",
    "@nestjs/swagger": "4.7.5",
    "@prisma/client": "2.13.0",
    "apollo-server-express": "2.19.0",
    "bcrypt": "5.0.0",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "cls-hooked": "4.2.2",
    "graphql": "15.4.0",
    "graphql-tools": "7.0.2",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.3",
    "swagger-ui-express": "4.1.5",
    "uuid": "8.3.2",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@apollo/gateway": "0.21.4",
    "@nestjs/cli": "7.5.4",
    "@nestjs/testing": "7.6.1",
    "@prisma/cli": "2.13.0",
    "@types/bcrypt": "3.0.0",
    "@types/chance": "1.1.0",
    "@types/cls-hooked": "4.3.3",
    "@types/express": "4.17.8",
    "@types/jest": "26.0.19",
    "@types/node": "13.13.5",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "chance": "1.1.7",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-prettier": "3.2.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "prisma-dbml-generator": "0.4.0",
    "rimraf": "3.0.2",
    "supertest": "5.0.0",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.4",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.0.5"
  }
}
